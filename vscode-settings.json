{
  "[markdown]": {
    "editor.fontSize": 13,
    "editor.minimap.enabled": false,
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "cSpell.userWords": ["tweepy"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "string.quoted.other.footnote",
        "settings": {
          "foreground": "#4b4b4b",
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "eslint.enable": true,
  "explorer.confirmDragAndDrop": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true,
    "suppressWelcomeNotice": true
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.keymap": "chorded",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": false,
  "jshint.enable": false,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.preview.autoShowPreviewToSide": false,
  "markdown.extension.showExplorer": true,
  "markdown.extension.toc.githubCompatibility": true,
  "markdown.extension.toc.updateOnSave": false,
  "markdownIndex.indexBase": "-",
  "markdownlint.run": "onType",
  "python.dataScience.askForKernelRestart": false,
  "spellright.documentTypes": ["latex", "markdown", "plaintext"],
  "spellright.language": ["de", "en"],
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
  "terminal.integrated.fontSize": 10,
  "terminal.integrated.rendererType": "dom",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "editorOverviewRuler.errorForeground": "#272A35",
    "editorOverviewRuler.warningForeground": "#272A35",
    "editorOverviewRuler.commonContentForeground": "#272A35",
    "editorOverviewRuler.currentContentForeground": "#272A35",
    "editorOverviewRuler.infoForeground": "#272A35",
    "editorOverviewRuler.incomingContentForeground": "#272A35",
    "editorOverviewRuler.addedForeground": "#272A35",
    "editorOverviewRuler.modifiedForeground": "#272A35",
    "editorOverviewRuler.deletedForeground": "#272A35",
    "editorGutter.addedBackground": "#272A35",
    "editorGutter.modifiedBackground": "#272A35",
    "editorGutter.deletedBackground": "#272A35"
  },
  "workbench.colorTheme": "Dracula",
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.hideActivityBar": true,
  "zenMode.hideLineNumbers": false,
  "workbench.sideBar.location": "right",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "python.linting.flake8Args": [
    "--ignore=E501"
  ]
}
